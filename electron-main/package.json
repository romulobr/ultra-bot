{
  "name": "ultrav3",
  "version": "0.4.2",
  "private": true,
  "author": "romulino",
  "description": "super vaca v3 - ultra",
  "dependencies": {
    "@feathersjs/authentication": "^2.1.11",
    "@feathersjs/authentication-jwt": "^2.0.5",
    "@feathersjs/authentication-oauth2": "^1.2.4",
    "@feathersjs/configuration": "^2.0.4",
    "@feathersjs/errors": "^3.3.4",
    "@feathersjs/express": "^1.2.7",
    "@feathersjs/feathers": "^3.2.3",
    "@feathersjs/socketio": "^3.2.6",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cors": "^2.8.4",
    "electron-window-state": "^5.0.2",
    "feathers-localstorage": "^3.0.0",
    "feathers-mongodb": "^3.3.0",
    "feathers-nedb": "^3.1.0",
    "feedme": "^1.2.0",
    "feedparser": "^2.2.9",
    "feedparser-promised": "^2.0.0",
    "googleapis": "^37.2.0",
    "helmet": "^3.14.0",
    "htmlparser2": "^3.10.0",
    "iconv-lite": "^0.4.24",
    "mongodb": "^3.1.8",
    "mongodb-core": "^3.1.7",
    "nedb": "^1.8.0",
    "passport-google-oauth20": "^1.0.0",
    "passport-twitch.js": "0.0.2",
    "passport-youtube-v3": "^2.1.0",
    "serve-favicon": "^2.5.0",
    "sinon": "^7.0.0",
    "socket.io": "^2.1.1",
    "twitch-js": "^1.2.17",
    "utf8": "^3.0.0",
    "winston": "^3.1.0"
  },
  "scripts": {
    "electron": "electron src/electron-starter.js",
    "electron-dev": "electron src/electron-starter-dev.js",
    "start": "electron src/electron-starter.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "mocha": "mocha server/test/ --recursive --exit",
    "watch": "mocha server/test/ --recursive --watch"
  },
  "build": {
    "appId": "com.romulino.v3.app",
    "directories": {
      "buildResources": "./assets/icons"
    },
    "files": [
      "./index.js",
      "./src/**/*",
      "./config/**/*",
      "./public-dist/**/*"
    ],
    "win": {
      "target": "NSIS"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "devDependencies": {
    "electron": "^3.0.6",
    "electron-builder": "^20.31.2",
    "electron-devtools-installer": "^2.2.4",
    "electron-redux-devtools": "^0.3.4",
    "chai": "^4.2.0",
    "eslint": "^5.7.0",
    "expect.js": "^0.3.1",
    "mocha": "^5.2.0",
    "nodemon": "1.18.8",
    "request": "^2.88.0",
    "request-promise": "^4.2.2"
  }
}
